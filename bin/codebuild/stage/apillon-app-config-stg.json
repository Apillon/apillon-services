{
  "name": "apillon-app-staging",
  "environment": {
    "computeType": "BUILD_GENERAL1_SMALL",
    "imagePullCredentialsType": "CODEBUILD",
    "privilegedMode": false,
    "image": "aws/codebuild/amazonlinux2-x86_64-standard:4.0",
    "environmentVariables": [
      {
        "type": "PLAINTEXT",
        "name": "ENV",
        "value": "staging"
      },
      {
        "type": "PLAINTEXT",
        "name": "S3_BUCKET",
        "value": "s3://app-staging.apillon.io"
      },
      {
        "type": "PLAINTEXT",
        "name": "CLOUDFRONT_ID",
        "value": "E1B1JK4NVJK9IP"
      }
    ],
    "type": "LINUX_CONTAINER"
  },
  "timeoutInMinutes": 60,
  "serviceRole": "arn:aws:iam::018021943180:role/atv2-deployer",
  "artifacts": {
    "type": "NO_ARTIFACTS"
  },
  "cache": {
    "type": "NO_CACHE"
  },
  "sourceVersion": "stage",
  "source": {
    "insecureSsl": false,
    "gitSubmodulesConfig": {
      "fetchSubmodules": false
    },
    "location": "https://TineMlakar@bitbucket.org/kalmiadevs/apillon-app.git",
    "gitCloneDepth": 1,
    "type": "BITBUCKET",
    "reportBuildStatus": false
  },
  "badgeEnabled": true,
  "queuedTimeoutInMinutes": 480,
  "logsConfig": {
    "s3Logs": {
      "status": "DISABLED",
      "encryptionDisabled": false
    },
    "cloudWatchLogs": {
      "status": "ENABLED",
      "groupName": "codebuild",
      "streamName": "apillon-app-staging"
    }
  },
  "encryptionKey": "arn:aws:kms:eu-west-1:018021943180:alias/aws/s3"
}